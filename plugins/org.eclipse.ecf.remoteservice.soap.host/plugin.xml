<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ecf.identity.namespace">
      <namespace
            class="org.eclipse.ecf.remoteservice.soap.identity.SoapNamespace"
            description="Soap WS Namespace"
            name="ecf.soap.ws.namespace">
      </namespace>
   </extension>
	<extension
	         point="org.eclipse.core.runtime.adapters">
	      <factory
	            adaptableType="org.eclipse.ecf.core.BaseContainer"
	            class="org.eclipse.ecf.remoteservice.soap.host.SoapServerContainerAdapterFactory">
	         <adapter type="org.eclipse.ecf.remoteservice.soap.host.ISoapServerContainerAdapter"/>
	      </factory>
   </extension>
</plugin>