<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

   <extension
         point="org.eclipse.ecf.containerFactory">
      <containerFactory
            class="org.eclipse.ecf.internal.provider.jms.activemq.ActiveMQJMSClientContainerInstantiator"
            description="ECF Generic Client - JMS (ActiveMQ)"
            name="ecf.jms.activemq.tcp.client">
      </containerFactory>
   </extension>

   <extension
         point="org.eclipse.ecf.containerFactory">
      <containerFactory
            class="org.eclipse.ecf.internal.provider.jms.activemq.ActiveMQJMSServerContainerInstantiator"
            description="ECF Generic Manager - JMS (ActiveMQ)"
            name="ecf.jms.activemq.tcp.manager">
      </containerFactory>
   </extension>

   <extension
         id="org.eclipse.ecf.provider.jms.ActiveMQServer"
         name="ECF Generic Server Application - JMS (ActiveMQ)"
         point="org.eclipse.core.runtime.applications">
      <application
            thread="main"
            visible="true">
         <run
               class="org.eclipse.ecf.provider.jms.activemq.application.ActiveMQJMSServer">
         </run>
      </application>
   </extension>

   <extension
         point="org.eclipse.core.runtime.adapters">
      <factory
            adaptableType="org.eclipse.ecf.provider.jms.activemq.container.ActiveMQJMSClientContainer"
            class="org.eclipse.ecf.provider.datashare.DatashareContainerAdapterFactory">
         <adapter type="org.eclipse.ecf.datashare.IChannelContainerAdapter"/>
      </factory>
   </extension>

   <extension
         point="org.eclipse.core.runtime.adapters">
      <factory
            adaptableType="org.eclipse.ecf.provider.jms.activemq.container.ActiveMQJMSServerContainer"
            class="org.eclipse.ecf.provider.datashare.DatashareContainerAdapterFactory">
         <adapter type="org.eclipse.ecf.datashare.IChannelContainerAdapter"/>
      </factory>
   </extension>

   <extension
         point="org.eclipse.core.runtime.adapters">
      <factory
            adaptableType="org.eclipse.ecf.provider.jms.activemq.container.ActiveMQJMSClientContainer"
            class="org.eclipse.ecf.provider.remoteservice.generic.RemoteServiceContainerAdapterFactory">
         <adapter type="org.eclipse.ecf.remoteservice.IRemoteServiceContainerAdapter"/>
      </factory>
   </extension>

   <extension
         point="org.eclipse.core.runtime.adapters">
      <factory
            adaptableType="org.eclipse.ecf.provider.jms.activemq.container.ActiveMQJMSServerContainer"
            class="org.eclipse.ecf.provider.remoteservice.generic.RemoteServiceContainerAdapterFactory">
         <adapter type="org.eclipse.ecf.remoteservice.IRemoteServiceContainerAdapter"/>
      </factory>
   </extension>


</plugin>
