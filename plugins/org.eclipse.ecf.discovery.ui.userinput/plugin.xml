<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.commands">
      <command
            categoryId="org.eclipse.ecf.discovery.ui.userinput.commands.category"
            id="org.eclipse.ecf.discovery.ui.userinput.commands.manualLookupCommand"
            name="%command.name.0">
      </command>
      <category
            id="org.eclipse.ecf.discovery.ui.userinput.commands.category"
            name="%category.name.0">
      </category>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="org.eclipse.ecf.discovery.ui.userinput.handler.LookupHandler"
            commandId="org.eclipse.ecf.discovery.ui.userinput.commands.manualLookupCommand">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="org.eclipse.ecf.discovery.ui.userinput.commands.manualLookupCommand"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+L">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ecf.discovery.ui.DiscoveryView?after=additions">
            <command
                  commandId="org.eclipse.ecf.discovery.ui.userinput.commands.manualLookupCommand"
                  id="org.eclipse.ecf.discovery.ui.userinput.menus.manualLookupCommand"
                  mnemonic="S">
            </command>
      </menuContribution>
      <menuContribution
            locationURI="toolbar:org.eclipse.ecf.discovery.ui.DiscoveryView?after=additions">
            <command
                  commandId="org.eclipse.ecf.discovery.ui.userinput.commands.manualLookupCommand"
                  icon="icons/manual.png"
                  id="org.eclipse.ecf.discovery.ui.userinput.toolbars.manualLookupCommand"
                  tooltip="%lookupButtonTooltip">
            </command>
      </menuContribution>
   </extension>
    <extension
         point="org.eclipse.ecf.containerFactory">
      <containerFactory
            class="org.eclipse.ecf.discovery.ui.userinput.UserInputDiscoveryContainerInstantiator"
            description="User input discovery provider"
            name="ecf.discovery.userinput.locator">
      </containerFactory>
   </extension>
    <extension
          point="org.eclipse.ecf.identity.namespace">
       <namespace
             class="org.eclipse.ecf.discovery.ui.userinput.UserInputNamespace"
             name="ecf.namespace.UserInputNamespace">
       </namespace>
    </extension>
</plugin>
